input UserInput {
  username: String!
  hashedPassword: String!
  firstName: String!
  lastName: String!
  email: String!
  phoneNumber: String!
  favouriteTeam: ID
}

input UpdateUserInput {
  id: ID!
  user: UserInput!
}

type User {
  id: ID!
  username: String!
  firstName: String!
  lastName: String!
  email: String!
  phoneNumber: String!
  favouriteTeam: ID
}