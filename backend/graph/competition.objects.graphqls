# Types
type Competition {
  id: ID!
  name: String!
  startMatchday: Int!
  status: CompStatus!
  rounds: [Round!]!
  createdAt: Time!
  updatedAt: Time!
}

# Enums
enum CompStatus {
  OPEN
  IN_PROGRESS
  COMPLETE
}

# Inputs
input CreateCompetitionInput {
  name: String!
  startMatchday: Int!
}

# Responses
