# Types
type Competition {
  id: ID!
  name: String!
  startMatchday: Int!
  status: CompetitionStatus!
  rounds: [Round!]!
  createdAt: Time!
  updatedAt: Time!
}

# Enums
enum CompetitionStatus {
  OPEN
  IN_PROGRESS
  COMPLETE
}

# Inputs
input CreateCompetitionInput {
  name: String!
  startMatchday: Int!
}

# Responses
